{
    "name": "svelte-app",
    "version": "1.0.0",
    "scripts": {
        "dev": "run-p routify rollup",
        "dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
        "build": "npm run routify -- -b && rollup -c",
        "serve": "spassr --serve-spa --serve-ssr",
        "export": "routify export",
        "build:docker": "npm run build && ./scripts/docker/docker-build.sh",
        "deploy:now": "cd scripts/now && npm run deploy",
        "deploy:netlify": "cd scripts/netlify && npm run deploy",
        "rollup": "rollup -cw",
        "routify": "routify -D"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@fullhuman/postcss-purgecss": "^2.2.0",
        "@rollup/plugin-commonjs": "^11.1.0",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "autoprefixer": "^9.8.0",
        "babel-eslint": "^10.1.0",
        "concurrently": "^5.1.0",
        "cross-env": "^7.0.2",
        "del": "^5.1.0",
        "eslint": "^7.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-svelte3": "^2.7.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.10",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "ppid-changed": "^1.0.1",
        "prettier": "^2.0.5",
        "prettier-plugin-svelte": "^1.1.0",
        "rollup": "^2.6.1",
        "rollup-plugin-copy": "^3.3.0",
        "rollup-plugin-hot": "0.0.31",
        "rollup-plugin-livereload": "^1.2.0",
        "rollup-plugin-svelte": "^5.2.1",
        "rollup-plugin-svelte-hot": "^0.9.2",
        "rollup-plugin-terser": "^5.3.0",
        "spassr": "^1.0.2",
        "svelte-preprocess": "^3.7.4",
        "tailwindcss": "^1.4.6"
    },
    "dependencies": {
        "@sveltech/routify": "^1.5.9",
        "svelte": "^3.20.1"
    }
}
